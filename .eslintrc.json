{
  "extends": ["airbnb", "react-app", "prettier"],
  "plugins": ["prettier", "react-hooks"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-props-no-spreading": "off",
    "arrow-body-style": ["error", "as-needed"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}

